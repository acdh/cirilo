  <foxml:datastream CONTROL_GROUP="X" ID="WSDL" STATE="A" VERSIONABLE="true">
    <foxml:datastreamVersion CREATED="2011-02-24T18:00:22.355Z" FORMAT_URI="http://schemas.xmlsoap.org/wsdl/"
