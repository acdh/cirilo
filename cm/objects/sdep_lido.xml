      <foxml:xmlContent>
        <wsdl:definitions name="LIDO" targetNamespace="bmech" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
          xmlns:mime="http://schemas.xmlsoap.org/wsdl/mime/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap"
          xmlns:soapenc="http://schemas.xmlsoap.org/wsdl/soap/encoding" xmlns:this="bmech"
          xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
          <wsdl:types>
            <xsd:schema targetNamespace="bmech">
              <xsd:simpleType name="STRING">
                <xsd:restriction base="xsd:string"/>
              </xsd:simpleType>
            </xsd:schema>
          </wsdl:types>
          <wsdl:message name="getRequest">
            <wsdl:part name="STYLESHEET" type="this:STRING"/>
            <wsdl:part name="LIDO_SOURCE" type="this:STRING"/>
            <wsdl:part name="locale" type="this:STRING"/>
            <wsdl:part name="mode" type="this:STRING"/>
            <wsdl:part name="context" type="this:STRING"/>
          </wsdl:message>
          <wsdl:message name="getPDFRequest">
            <wsdl:part name="FO_STYLESHEET" type="this:STRING"/>
            <wsdl:part name="LIDO_SOURCE" type="this:STRING"/>
            <wsdl:part name="locale" type="this:STRING"/>
            <wsdl:part name="mode" type="this:STRING"/>
          </wsdl:message>
          <wsdl:message name="dissemResponse">
            <wsdl:part name="dissem" type="xsd:base64Binary"/>
          </wsdl:message>
          <wsdl:portType name="LIDOPortType">
            <wsdl:operation name="get">
              <wsdl:input message="this:getRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="getPDF">
              <wsdl:input message="this:getPDFRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
          </wsdl:portType>
          <wsdl:service name="LIDO">
            <wsdl:port binding="this:LIDO_http" name="LIDO_port">
              <http:address location="http://fedora.host/cocoon/"/>
            </wsdl:port>
          </wsdl:service>
          <wsdl:binding name="LIDO_http" type="this:LIDOPortType">
            <http:binding verb="GET"/>
            <wsdl:operation name="get">
              <http:operation location="toHTML?source=(LIDO_SOURCE)&amp;style=(STYLESHEET)&amp;model=LIDO&amp;locale=(locale)&amp;mode=(mode)&amp;context=(context)"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/html"/>
              </wsdl:output>
            </wsdl:operation>
            <wsdl:operation name="getPDF">
              <http:operation location="toPDF?source=(LIDO_SOURCE)&amp;style=(FO_STYLESHEET)&amp;model=LIDO&amp;locale=(locale)&amp;mode=(mode)"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="application/pdf"/>
              </wsdl:output>
            </wsdl:operation>
          </wsdl:binding>
        </wsdl:definitions>
      </foxml:xmlContent>
    </foxml:datastreamVersion>
