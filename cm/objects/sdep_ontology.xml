              </xsd:simpleType>
            </xsd:schema>
          </wsdl:types>
          <wsdl:message name="getOntologyRequest">
            <wsdl:part name="query" type="this:STRING"/>
          </wsdl:message>
          <wsdl:message name="getRequest">
            <wsdl:part name="STYLESHEET" type="this:STRING"/>
            <wsdl:part name="ONTOLOGY" type="this:STRING"/>
            <wsdl:part name="locale" type="this:STRING"/>
            <wsdl:part name="mode" type="this:STRING"/>
            <wsdl:part name="context" type="this:STRING"/>
          </wsdl:message>
          <wsdl:message name="getPDFRequest">
            <wsdl:part name="FO_STYLESHEET" type="this:STRING"/>
            <wsdl:part name="ONTOLOGY" type="this:STRING"/>
            <wsdl:part name="locale" type="this:STRING"/>
            <wsdl:part name="mode" type="this:STRING"/>
          </wsdl:message>
          <wsdl:message name="dissemResponse">
            <wsdl:part name="dissem" type="xsd:base64Binary"/>
          </wsdl:message>
          <wsdl:portType name="OntologyPortType">
            <wsdl:operation name="getSPARQL">
              <wsdl:input message="this:getOntologyRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="get">
              <wsdl:input message="this:getRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
            <wsdl:operation name="getPDF">
              <wsdl:input message="this:getPDFRequest"/>
              <wsdl:output message="this:dissemResponse"/>
            </wsdl:operation>
          </wsdl:portType>
          <wsdl:service name="Ontology">
            <wsdl:port binding="this:Ontology_http" name="Ontology_port">
              <http:address location="http://fedora.host/cocoon/"/>
            </wsdl:port>
          </wsdl:service>
          <wsdl:binding name="Ontology_http" type="this:OntologyPortType">
            <http:binding verb="GET"/>
            <wsdl:operation name="getSPARQL">
              <http:operation location="getSPARQL?query=(query)"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/html"/>
              </wsdl:output>
            </wsdl:operation>
            <wsdl:operation name="get">
              <http:operation location="toHTML?source=(ONTOLOGY)&amp;style=(STYLESHEET)&amp;locale=(locale)&amp;mode=(mode)"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="text/html"/>
              </wsdl:output>
            </wsdl:operation>
            <wsdl:operation name="getPDF">
              <http:operation location="toPDF?source=(ONTOLOGY)&amp;style=(FO_STYLESHEET)&amp;locale=(locale)&amp;mode=(mode)"/>
              <wsdl:input>
                <http:urlReplacement/>
              </wsdl:input>
              <wsdl:output>
                <mime:content type="application/pdf"/>
              </wsdl:output>
            </wsdl:operation>
          </wsdl:binding>
        </wsdl:definitions>
      </foxml:xmlContent>
